<Group labelreference="L2" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">romberg := proc(f, a, b, EPSILON)
    local T, T_j, h_k, i, j, k, m, n, values, temp, sum;
    T := Array([]);
    k := 2;
    sum := 0;
    
    for j from 0 to k-1 do
        values := Array([]);
	h_k := evalf((b-a)/2^j);
	n := 2^j;
	
        for m from 0 to n do
	    Append(values, f(evalf((m * h_k) + a)));
	od;
	
	for i from 2 to n do
	    sum := sum + values[i];
	od;
	
        T_j := evalf(h_k / 2 * ((values[1] + values[n + 1]) + 2 * sum));
	Append(T, [T_j]);
    od;

    T := aux(T, k);
    while(abs(T[k][k] - T[k-1][k-1]) &gt; EPSILON) and (abs(T[k][k-1] - T[k-1][k-1]) &gt; EPSILON) do:
	values := Array([]);
        k := k + 1;
	h_k := evalf((b - a) / 2^(k - 1) );
	n := 2^(k - 1);
	
        for m from 0 to n do
	    Append(values, f(evalf(a+ m * h_k)));
	od;
	
        sum := 0;
	for i from 2 to n do
	    sum := sum + values[i];
        od;
		
        T_j := evalf(h_k / 2 * ((values[1] + values[n + 1]) + 2 * sum));
	Append(T, [T_j]);
	T := aux(T, k);
    od;

    return T, T[k][k], k; 

end proc:
</Text-field>
</Input>
</Group>
<Group hide-input="false" labelreference="L3" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">aux := proc(T, y):
    local a, b, x;
    b := T;
    for x from 2 to y do:
        a := evalf((4 ^ (x - 1) * T[y][x - 1] - T[y - 1][x - 1])/(4^(x - 1) - 1));
	b[y] := Append(T[y], a, inplace = false);
    od;
    return b;

end proc:
</Text-field>
</Input>
</Group>
