<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">horner := proc(polynomial, x0::integer)
    local poly, i, expr, result, adds, muls;
    poly := [Physics[Coefficients](polynomial, x)]; # List that stores all the coefficients of the given polynomial.
    expr := poly[1];              # Stores the result of the polynomial evaluated at x.
    adds := 0;                    # Stores the amount of addition operations that have occured.
    muls := 0;                    # Stores the amount of multiplication operations that have occured.
    
    for i from 2 to nops(poly) do
        expr := expr*x + poly[i];
        adds := adds + 1;
        muls := muls + 1;
    
    end do;
<Font encoding="UTF-8">    printf(&quot;\134n%a can be transformed into %a&quot;, polynomial, expr);
</Font>    result := subs(x=x0,expr);
<Font encoding="UTF-8">    printf(&quot;\134nThe result for the given polynomial for x = %d is %d.\134n&quot;, x0, result</Font>);
<Font encoding="UTF-8">    printf(&quot;This computation took %d additions and %d multiplications.\134n&quot;, adds, muls);</Font>
end proc;

</Text-field>
